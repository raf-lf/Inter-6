  m_Script: {fileID: 11500000, guid: ac93e3151d371d54b9c2242daf660070, type: 3}
  m_Name: Farmer
  m_EditorClassIdentifier: 
  lines:
  - actor: {fileID: 11400000, guid: f70e747e88b6dd64db42f04789c1ae39, type: 2}
    emotion: 2
    portraitPosition: 1
    line: Que friiiiiiiiiiiiiio!
    lineEnglish: 
  - actor: {fileID: 11400000, guid: 48c13206a3c2a2c4ca6e27faf2af1056, type: 2}
    emotion: 4
    portraitPosition: 0
    line: "Hm... Pare pra prestar aten\xE7\xE3o. Voc\xEA realmente ta sentindo algo?"
    lineEnglish: 
  - actor: {fileID: 11400000, guid: f70e747e88b6dd64db42f04789c1ae39, type: 2}
    emotion: 3
    portraitPosition: 1
    line: "Hm... Acho que n\xE3o... Mas esse lugar n\xE3o tem sol! Deveria estar
      congelando, n\xE3o?"
    lineEnglish: 
  - actor: {fileID: 11400000, guid: 48c13206a3c2a2c4ca6e27faf2af1056, type: 2}
    emotion: 0
    portraitPosition: 0
    line: "Olha, o clima n\xE3o \xE9 l\xE1 muito relevante por essas bandas..."
    lineEnglish: 
  - actor: {fileID: 11400000, guid: f70e747e88b6dd64db42f04789c1ae39, type: 2}
    emotion: 3
    portraitPosition: 1
    line: "Mas e minha planta\xE7\xE3o de batatas? Como ela vai sobreviver?"
    lineEnglish: 
  - actor: {fileID: 11400000, guid: 48c13206a3c2a2c4ca6e27faf2af1056, type: 2}
    emotion: 3
    portraitPosition: 0
    line: "Que batatas, amigo? Nesse lugar s\xF3 h\xE1 lama."
    lineEnglish: 
  - actor: {fileID: 11400000, guid: f70e747e88b6dd64db42f04789c1ae39, type: 2}
    emotion: 1
    portraitPosition: 1
    line: "Oh... Voc\xEA tem raz\xE3o... Minha fazenda foi levada por ela..."
    lineEnglish: 
  - actor: {fileID: 11400000, guid: f70e747e88b6dd64db42f04789c1ae39, type: 2}
    emotion: 4
    portraitPosition: 1
    line: E agora? A vila depende de mim pra comer!
    lineEnglish: 
  - actor: {fileID: 11400000, guid: 48c13206a3c2a2c4ca6e27faf2af1056, type: 2}
    emotion: 1
    portraitPosition: 0
    line: "Voc\xEA n\xE3o precisa mais se preocupar com isso, amigo. Ta na hora de
      voc\xEA descansar."
    lineEnglish: 
  - actor: {fileID: 11400000, guid: f70e747e88b6dd64db42f04789c1ae39, type: 2}
    emotion: 2
    portraitPosition: 1
    line: "Descansar... Meus bra\xE7os est\xE3o cansados de tanto arar a terra...
      Minhas costas doem de tanto arder no sol... Minha garganta seca de n\xE3o ter
      \xE1gua pra beber."
    lineEnglish: 
  - actor: {fileID: 11400000, guid: f70e747e88b6dd64db42f04789c1ae39, type: 2}
    emotion: 1
    portraitPosition: 1
    line: "Pois preste aten\xE7\xE3o novamente... Ainda sente isso?"
    lineEnglish: 
  - actor: {fileID: 11400000, guid: f70e747e88b6dd64db42f04789c1ae39, type: 2}
    emotion: 1
    portraitPosition: 1
    line: "Eu... Eu n\xE3o sinto! Que maravilha!"
    lineEnglish: 
  - actor: {fileID: 11400000, guid: 48c13206a3c2a2c4ca6e27faf2af1056, type: 2}
    emotion: 1
    portraitPosition: 0
    line: "Vamos adiante. Voc\xEA j\xE1 trabalhou o suficiente pra uma vida inteira."
    lineEnglish: 
